{
  "manifest_version": 3,
  "name": "Bookmark Search and Navigation",
  "version": "1.0",
  "description": "快速搜索和导航书签。",
  "permissions": ["bookmarks", "storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["navigation.html", "navigation.js", "style.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
