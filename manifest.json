{
  "manifest_version": 3,
  "name": "书签管理器",
  "version": "1.0",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"  // 指定为 popup.html 页面
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
