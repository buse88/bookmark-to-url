{
  "manifest_version": 3,
  "name": "Bookmark to URL",
  "version": "1.0",
  "description": "An example extension to process bookmarks.",
  "permissions": [
    "contextMenus",
    "storage",
    "bookmarks"  // 添加书签权限
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
 