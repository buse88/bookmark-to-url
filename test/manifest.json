{
  "name": "Bookmark Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
